{
  "name": "streaming",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "lint": "next lint --fix",
    "preview": "vite preview",
    "prettier": "prettier -c .prettierrc \"src/**/**/*.{js,jsx,ts,tsx}\" --write",
    "pre-commit": "npm run prettier && npm run lint && npx --no-install lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "git add ."
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.7",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.4",
    "@mui/styled-engine-sc": "^5.11.0",
    "@types/history": "^5.0.0",
    "caller": "^1.1.0",
    "framer-motion": "^8.4.1",
    "husky": "^8.0.3",
    "jquery": "^3.6.3",
    "lint": "^0.8.19",
    "mui": "^0.0.1",
    "nipplejs": "^0.10.0",
    "phaser": "^3.55.2",
    "phaser3": "^0.4.34",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-devtools-core": "^4.27.1",
    "react-dom": "^18.2.0",
    "react-joystick-component": "^6.0.0",
    "react-router-dom": "^6.7.0",
    "redux": "^4.2.0",
    "socket.io-client": "^4.5.4",
    "styled-components": "^5.3.6",
    "type-fest": "^3.5.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/caller": "^1.0.0",
    "@types/jquery": "^3.5.16",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^3.0.0",
    "caller-id": "^0.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-mkcert": "^1.11.0"
  }
